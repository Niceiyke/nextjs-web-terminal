# Server Configuration
PORT=3000
NODE_ENV=development

# Session Secret (must be at least 32 characters)
SESSION_SECRET=change-this-to-a-random-32-character-secret-key-in-production

# Encryption Key for database passwords (must be at least 32 characters)
ENCRYPTION_KEY=change-this-to-a-random-32-character-encryption-key

# Web Authentication
WEB_AUTH_ENABLED=true
WEB_USERNAME=admin
WEB_PASSWORD=change-this-password
